<%- include('./partials/header.ejs') %>
<body>
  <h1>Search Results</h1>
  <%- include('./partials/nav') %>
  <ul class="results">
    <% results.forEach(result => { %>
      <% if (result.results) { %>
        <% result.results.forEach(doc => { %>
          <li><%= doc.name %> - <%= doc.quantity %> - <%= doc.strength %> - <%= doc.price %></li>
        <% }) %>
      <% } else { %>
        <li><%= result.name %> - <%= result.quantity %> - <%= result.strength %> - <%= result.price %></li>
      <% } %>
    <% }) %>
  </ul>
</body>